#!/usr/bin/env python3
import argparse
from datetime import datetime, timedelta


def main():
    parser = argparse.ArgumentParser(description="Print dates from start to end")
    parser.add_argument("start", type=str, help="Start date in ISO format (YYYY-MM-DD)")
    parser.add_argument("end", type=str, help="End date in ISO format (YYYY-MM-DD)")

    args = parser.parse_args()

    try:
        start_date = datetime.strptime(args.start, "%Y-%m-%d").date()
        end_date = datetime.strptime(args.end, "%Y-%m-%d").date()
    except ValueError:
        parser.error("Dates must be in ISO format (YYYY-MM-DD)")

    current_date = start_date
    while current_date <= end_date:
        print(current_date.isoformat())
        current_date += timedelta(days=1)


if __name__ == "__main__":
    main()
